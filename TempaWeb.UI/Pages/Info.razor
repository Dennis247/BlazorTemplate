@page "/info"
@layout PageLayout

<section class="sign-in-page">
    <div class="container p-0">
        <div class="row no-gutters">
            <div class="col-sm-12 align-self-center">
                <div class="sign-in-from bg-white">
                    <img src="images/login/mail.png" width="80" alt="">
                    <h1 class="mt-3 mb-0">@infoMessageServices.Title !</h1>
                    <p>@infoMessageServices.Message</p>
                    <div class="d-inline-block w-100">
                   
                        <button @onclick="Navigate"  class="btn btn-primary mt-3">Back to Home</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

